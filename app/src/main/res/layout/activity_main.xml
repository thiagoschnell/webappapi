<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
  <Button
      android:id="@+id/ButtonOpenActivity2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Single Request"></Button>
  <Button
      android:id="@+id/ButtonOpenActivity3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="On Demand Requests"></Button>
  <Button
      android:id="@+id/ButtonOpenActivity3Parallel"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="On Demand Parallel Requests"></Button>
</LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>